<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.forms.WarehouseActivity">

    <EditText
        android:id="@+id/warehouse_purchaseId_edittext"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="356dp"
        android:text="PurchaseId"
        app:layout_constraintBottom_toTopOf="@+id/back_toSignedActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/get_all_warehouses"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="POKAŻ WSZYSTKIE"
        app:layout_constraintBottom_toBottomOf="@+id/save_warehouse"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/warehouse_purchaseId_edittext" />

    <Button
        android:id="@+id/save_warehouse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="ZAPISZ"
        app:layout_constraintBottom_toTopOf="@+id/back_toSignedActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/get_all_warehouses"/>

    <Button
        android:id="@+id/back_toSignedActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="WRÓĆ"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/save_warehouse" />


</androidx.constraintlayout.widget.ConstraintLayout>